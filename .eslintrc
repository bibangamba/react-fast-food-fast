{
    "extends": "airbnb",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "rules": {
        "comma-dangle": ["error", "never"],
        "arrow-parens": 0,
        "object-curly-newline": 0,
        "implicit-arrow-linebreak": 0,
        "quotes": 0
    }
}
